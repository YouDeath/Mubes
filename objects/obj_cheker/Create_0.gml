function check(){
	
	for (i = 0; i < howChapters; i++){
		file = file_text_open_read("chapter"+string(i)+".txt")
		if file == -1{
			file = file_text_open_write("chapter"+string(i)+".txt")
			switch i{
				case 0:{
					file_text_write_string(file,"begining")
					file_text_close(file);
					break
				}
				case 1:{
					file_text_write_string(file,"simples")
					file_text_close(file);
					break
				}
				case 2:{
					file_text_write_string(file,"something new")
					file_text_close(file);
					break
				}
			}
			for (j = 0; j < howLevel; j++){
				file = file_text_open_write("level"+string(i)+"-"+string(j)+".txt")
				level(i*25+j)
				for (k = 0; k < 6; k++){
					for (l = 0; l < 6; l++){
						file_text_write_real(file,map[k,l])
					}
				}
				for (k = 0; k < 6; k++){
					for (l = 0; l < 6; l++){
						file_text_write_real(file,mapNumberMask[k,l])
					}
				}
				for (k = 0; k < 4; k++){
					file_text_write_real(file,tasks[0,k])
					file_text_write_real(file,tasks[1,k])
				}
				file_text_close(file)
				file = file_text_open_write("level"+string(i)+"-"+string(j)+"-info.txt")
				if true{ //i+j == 0
					file_text_write_real(file,1)
				}
				else{
					file_text_write_real(file,0)
				}
				file_text_close(file)
			}
		}
		else{
			show_debug_message("!!!!!!"+file_text_read_string(file))
			file_text_close(file)
		}
	}
}

